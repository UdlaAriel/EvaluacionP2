<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AnchapaxiArielP2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AnchapaxiArielP2.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="Recarga telefónica">

    <TabBar>
        <ShellContent
        Title="Menú principal"
        ContentTemplate="{DataTemplate local:MainPage}"
        Icon="dotnet_bot.png"
        Route="MainPage" />
        <ShellContent
        Title="Recarga Telefónica"
        ContentTemplate="{DataTemplate local:TelephoneRechargePage}"
            Icon="dotnet_bot.png"
        Route="TelephoneRechargePage" />
        <ShellContent
        Title="Acerca"
        ContentTemplate="{DataTemplate local:AboutPage}"
            Icon="dotnet_bot.png"
        Route="AboutPage" />
    </TabBar>

</Shell>
